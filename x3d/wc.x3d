<!DOCTYPE html>

<html>
<head>
	<script type='text/javascript' src='https://www.x3dom.org/download/x3dom.js'> </script> 
    <link rel='stylesheet' type='text/css' href='https://www.x3dom.org/download/x3dom.css'></link> 
</head>

<body>


<X3D width="1920" height="1080">

<Scene>


<Transform scale="0.6,0.2,0.6">
	<Shape>
		<Appearance>
			<Material diffuseColor="1.000, 0.973, 0.863" ></Material>
		</Appearance>
		<Sphere></Sphere>
	</Shape>
</Transform>

<!-- Picior -->
<Transform translation='0,-0.2,0'>
	<Transform scale="0.3,0.3,0.3">
		<Shape>
			<Appearance>
				<Material diffuseColor="1.000, 0.973, 0.863" ></Material>
			</Appearance>
			<Cylinder></Cylinder>
		</Shape>
	</Transform>
</Transform>

<!-- spatar -->
<Transform translation='0,0.4,-0.457'>
	<Transform rotation='1,0,0,1.5708'> 
		<Transform scale="0.6,0.2,0.6">
			<Shape>
				<Appearance>
					<Material diffuseColor="0.824, 0.706, 0.549" ></Material>
				</Appearance>
				<Sphere></Sphere>
			</Shape>
		</Transform>
	</Transform>
</Transform>

<!-- flusher -->
<Transform translation='0,0.4,-0.75'>
	<Transform scale="0.4,0.5,0.2">
		<Shape>
			<Appearance>
				<Material diffuseColor="0.863, 0.863, 0.863" ></Material>
			</Appearance>
			<Box></Box>
		</Shape>
	</Transform>
</Transform>
	
</Scene>

</X3D>

</body>
</html>
